{
  "name": "manabie-todo-challenge",
  "description": "Manabie todo challenge",
  "author": "John Carlo Dimaranan",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "cross-env": "^7.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "shortid": "^2.2.15",
    "styled-components": "^5.3.5"
  },
  "scripts": {
    "start:fullstack": "cross-env REACT_APP_WHOAMI=fullstack react-scripts start",
    "start:frontend": "cross-env REACT_APP_WHOAMI=frontend react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "test": "jest --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.4.0",
    "@types/node": "^12.0.0",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^16.9.0",
    "@types/shortid": "^0.0.29",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "27.0.6",
    "ts-jest": "^28.0.0",
    "typescript": "4.5.3"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "verbose": true,
    "testURL": "http://localhost/",
    "setupFilesAfterEnv": [
      "./src/setupTests.ts"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    }
  }
}
